import pyttsx3
engine = pyttsx3.init()
engine.setProperty('rate', 160)  # slower rate
engine.setProperty('volume', 1.0)  # max volume
voices = engine.getProperty('voices')
for v in voices:
    print(v.name)  # Try different voices
engine.setProperty('voice', voices[25].id)  # pick a better one
engine.say("Next turn in 5 meters")
engine.runAndWait()
